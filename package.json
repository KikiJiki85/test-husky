{
  "name": "puzzle",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.base.config.js",
  "scripts": {
    "start": "webpack serve --open --config ./webpack.config.js --env mode=dev",
    "build": "webpack --config ./webpack.config.js --env mode=prod",
    "lint": "eslint \"src/*.{ts,tsx}\"",
    "format": "prettier --write ./src",
    "ci:format": "prettier --check ./src",
    "prepare": "husky"
  },
  "validate-branch-name": {
    "pattern": "^(rss-puzzle|master)$|^(init|feat|fix|refactor|docs)\\/RSS-PZ-[0-9]{2}_[a-zA-Z]+$",
    "errorMsg": "Working branch name is not correct, please fix it"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "prettier": "3.2.5",
    "style-loader": "^3.3.4",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.0",
    "webpack-merge": "^5.10.0"
  }
}
